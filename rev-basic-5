a=[0xAD, 0xD8, 0xCB,0xCB, 0x9D, 0x97, 0xCB, 0xC4,0x92, 0xA1, 0xD2, 0xD7, 0xD2, 0xD6, 0xA8, 0xA5, 0xDC, 0xC7,0xAD, 0xA3, 0xA1, 0x98, 0x4C,0,0,0]
result=0

for j in range(0,24):
  for i in range(j,24):
    if j%2==0:
      result+=pow(-1,24-i)*a[i]
    else:
      result+=pow(-1,23-i)*a[i]
      
  if result<0:
    result=-result
  print(chr(result),end="")
  result=0
