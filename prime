//소수 배열에 저장
void prime(int *pr,int size)
{
	
	for (int i = 0; i < size; i++)//주어진 소수 범위+1
		pr[i] = 0;
	int i, j, cnt;
		for(i = 3;i <= size-1; i += 2) {
		for (j = 3, cnt = 0; j * j <= i; j += 2) {
			if (i % j == 0)
			{
				cnt++;
				break;
			}
		}
		if (cnt == 0)
			pr[i] = i;
	}
		pr[2] = 2;
}
