void qsort(int *arr, int start, int end)
{
	if(start>=end)
	{
		return;
	}
	
	int pivot = start;	
	int i = pivot+1;	
	int j = end;	
	int temp;
	
	while(i<=j)
	{
		while(i<=end && arr[i]<=arr[pivot])
		{
			++i;
		}
		while(j>start && arr[j]>=arr[pivot])
		{
			--j;
		}
		if(i>=j) break;
		temp = arr[i];
		arr[i] = arr[j];
		arr[j] = temp;
	}
	
	temp = arr[j];
	arr[j] = arr[pivot];
	arr[pivot] = temp;
	
	qsort(arr, start, j-1);	
	qsort(arr, j+1, end);	
	
}
